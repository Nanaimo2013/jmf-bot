    "install:deps": "node src/managers/install/dependencies.js",
    "install:config": "node src/managers/install/config.js",
    
    "update:manager": "node src/managers/update/index.js",
    "update:check": "node src/managers/update/check.js",
    "update:backup": "node src/managers/update/backup.js",
    "update:migrate": "node src/managers/update/migrate.js",
    
    "test:manager": "node src/managers/test/index.js",
    "test:unit": "node src/managers/test/unit.js",
    "test:integration": "node src/managers/test/integration.js",
    "test:e2e": "node src/managers/test/e2e.js",
    
    "monitor:manager": "node src/managers/monitor/index.js",
    "monitor:start": "node src/managers/monitor/start.js",
    "monitor:stop": "node src/managers/monitor/stop.js",
    "monitor:status": "node src/managers/monitor/status.js",
    
    "docker:manager": "node src/managers/docker/index.js",
    "docker:build": "node src/managers/docker/build.js",
    "docker:start": "node src/managers/docker/start.js",
    "docker:stop": "node src/managers/docker/stop.js",
    "docker:logs": "node src/managers/docker/logs.js",
    
    "db:manager": "node src/managers/database/index.js",
    "db:init": "node src/managers/database/init.js",
    "db:migrate": "node src/managers/database/migrate.js",
    "db:backup": "node src/managers/database/backup.js",
    "db:restore": "node src/managers/database/restore.js",
    
    "deploy": "node src/deploy-commands.js",
    "deploy:global": "node src/deploy-commands.js --global",
    "deploy:guild": "node src/deploy-commands.js --guild",
    
    "register": "node register-commands.js",
    "register:global": "node register-commands.js --global",
    "register:guild": "node register-commands.js --guild",
    "register:all": "node register-commands.js --all",
    
    "lint": "eslint .",
