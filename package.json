{
  "name": "jmf-bot",
  "version": "1.0.0",
  "description": "Custom Discord bot for JMF Hosting server",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "stop": "node scripts/stop.js",
    "dev": "nodemon src/index.js",
    "deploy": "node src/deploy-commands.js",
    "deploy:global": "node src/deploy-commands.js --global",
    "deploy:guild": "node src/deploy-commands.js --guild",
    "register": "node register-commands.js",
    "register:global": "node register-commands.js --global",
    "register:guild": "node register-commands.js --guild",
    "register:all": "node register-commands.js --all",
    "backup": "node src/utils/backup.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"No tests specified - skipping tests\" && exit 0",
    "db:fix": "node scripts/database/fix-database.js",
    "db:update": "bash update-database.sh",
    "db:init": "node src/database/init.js",
    "set-perms": "node scripts/set-permissions.js",
    "update": "sudo ./scripts/update/updater.sh", 
    "docker:build": "docker-compose build",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "discord",
    "bot",
    "jmf",
    "hosting",
    "game",
    "server"
  ],
  "author": "JMFHosting (https://github.com/Nanaimo2013)",
  "license": "MIT",
  "copyright": "Â© 2025 JMFHosting. All Rights Reserved.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nanaimo2013/Jmf-Bot.git"
  },
  "homepage": "https://www.jmfhosting.com",
  "bugs": {
    "url": "https://github.com/Nanaimo2013/Jmf-Bot/issues"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2",
    "moment": "^2.29.4",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "cron": "^4.1.0",
    "node-cron": "^3.0.2",
    "express": "^4.18.2",
    "axios": "^1.6.2",
    "canvas": "^2.11.2",
    "chart.js": "^3.9.1",
    "chartjs-node-canvas": "^4.1.6",
    "fs-extra": "^11.2.0",
    "sqlite3": "^5.1.6",
    "sqlite": "^5.0.1",
    "eslint": "^9.22.0",
    "nodemon": "^3.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^9.22.0"
  },
  "engines": {
    "node": ">=16.9.0"
  }
} 