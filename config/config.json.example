  "api": {
    "enabled": true,
    "port": 3000,
    "host": "localhost",
    "prefix": "/api",
    "rateLimit": {
      "windowMs": 900000,
      "max": 100
    },
    "secretKey": "YOUR_API_SECRET_KEY_HERE",
    "cors": {
      "enabled": true,
      "origin": "*",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    "routes": {
      "auth": true,
      "users": true,
      "servers": true,
      "stats": true
    }
  },
  "gameServers": {
    "statusChannel": "server-status",
    "updateInterval": 300,
    "pterodactyl": {
      "enabled": true,
      "apiKey": "YOUR_PTERODACTYL_API_KEY_HERE",
      "url": "https://panel.jmfhosting.com/api/client",
      "nodes": [
        {
          "name": "Node 1",
          "location": "US East",
          "id": 1
        }
      ]
    }
  },
  "monitoring": {
    "enabled": true,
    "interval": 60000,
    "webhookUrl": "YOUR_WEBHOOK_URL_HERE",
    "metrics": {
      "system": true,
      "discord": true,
      "database": true,
      "api": true
    },
    "alerts": {
      "discord": true,
      "email": false
    }
  },
  "backup": {
    "enabled": true,
    "interval": 86400000,
    "directory": "./backups",
    "maxAge": 604800000,
    "compress": true,
    "types": {
      "database": true,
      "config": true,
      "logs": false
    }
  },
  "features": {
    "tickets": true,
    "moderation": true,
    "gameServers": true,
    "analytics": true,
    "automod": true,
    "welcome": true,
    "leveling": true,
