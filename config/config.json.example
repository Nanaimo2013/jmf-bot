{
  "bot": {
    "token": "YOUR_BOT_TOKEN_HERE",
    "clientId": "YOUR_CLIENT_ID_HERE", 
    "guildId": "YOUR_GUILD_ID_HERE",
    "prefix": "!",
    "embedColor": "#00AAFF",
    "footerText": "JMF Hosting | Game Server Solutions",
    "owners": ["OWNER_ID_1", "OWNER_ID_2"],
    "admins": ["ADMIN_ID_1", "ADMIN_ID_2"],
    "sendWelcomeDM": true,
    "welcomeDMMessage": "Thanks for joining our server! If you need any help, feel free to create a ticket in our support channel."
  },
  "managers": {
    "enabled": true,
    "roles": ["Owner", "Manager"],
    "permissions": {
      "manageBot": true,
      "manageUsers": true,
      "manageServers": true,
      "viewAnalytics": true,
      "manageEconomy": true,
      "manageGames": true
    },
    "notifications": {
      "enabled": true,
      "channelId": "YOUR_CHANNEL_ID",
      "mentions": true,
      "events": {
        "serverStatus": true,
        "userReports": true,
        "systemAlerts": true,
        "backupStatus": true
      }
    },
    "dashboard": {
      "enabled": true,
      "port": 3000,
      "secret": "YOUR_DASHBOARD_SECRET"
    }
  },
  "database": {
    "type": "sqlite",
    "migrations": {
      "directory": "./src/database/migrations",
      "tableName": "migrations",
      "extension": "js"
    },
    "backup": {
      "enabled": true,
      "directory": "./backups/database",
      "interval": "1d",
      "maxBackups": 10,
      "compression": true
    },
    "connections": {
      "sqlite": {
        "filename": "./data/database.sqlite",
        "useNullAsDefault": true,
        "foreign_key_constraints": true
      }
    },
    "pool": {
      "min": 2,
      "max": 10,
      "idleTimeoutMillis": 30000
    }
  },
  "logging": {
    "level": "info",
    "format": "simple",
    "directory": "./logs",
    "console": true,
    "file": true,
    "maxSize": "10m",
    "maxFiles": 30,
    "categories": {
      "system": {
        "level": "debug",
        "filename": "system.log"
      },
      "commands": {
        "level": "info",
        "filename": "commands.log"
      },
      "events": {
        "level": "info",
        "filename": "events.log"
      },
      "errors": {
        "level": "error",
        "filename": "errors.log"
      },
      "database": {
        "level": "info",
        "filename": "database.log"
      },
      "api": {
        "level": "info",
        "filename": "api.log"
      }
    },
    "rotation": {
      "enabled": true,
      "interval": "1d",
      "maxFiles": 30
    }
  },
  "install": {
    "configDir": "./config",
    "dataDir": "./data",
    "logsDir": "./logs",
    "dependencies": [
      "discord.js",
      "express",
      "winston",
      "sqlite3",
      "mysql2"
    ],
    "setup": {
      "createDirectories": true,
      "installDependencies": true,
      "runMigrations": true,
      "generateConfig": true
    },
    "permissions": {
      "dataDir": "0755",
      "configFiles": "0644"
    }
  },
  "docker": {
    "enabled": true,
    "containerName": "jmf-bot",
    "imageName": "jmf-bot",
    "imageTag": "latest",
    "ports": {
      "3000": "3000"
    },
    "volumes": {
      "./data": "/app/data",
      "./logs": "/app/logs",
      "./config": "/app/config"
    },
    "env": {
      "NODE_ENV": "production"
    },
    "networks": {
      "default": "jmf-network",
      "subnet": "172.20.0.0/16"
    },
    "security": {
      "privileged": false,
      "capDrop": ["ALL"],
      "capAdd": ["NET_ADMIN"],
      "securityOpt": ["no-new-privileges"]
    },
    "logging": {
      "driver": "json-file",
      "options": {
        "max-size": "10m",
        "max-file": "3"
      }
    }
  },
  "welcomeSystem": {
    "enabled": true,
    "channelId": "YOUR_CHANNEL_ID",
    "channelName": "join-leave",
    "message": "Welcome to the JMF Hosting Discord server, {user}! We're glad to have you here. Please read our rules in {rules} and enjoy your stay!",
    "embedColor": "#00FF00",
    "mentionUser": true,
    "showJoinDate": true,
    "showAccountAge": true
  },
  "verification": {
    "enabled": true,
    "verifiedRole": "Member",
    "unverifiedRole": "Unverified",
    "message": "Welcome to the server! Please click the button below to verify yourself and gain access to the server.",
    "buttonText": "Verify",
    "successMessage": "You have been successfully verified! You now have access to the server.",
    "embedColor": "#00FF00",
    "sendWelcomeMessage": true,
    "welcomeChannel": "welcome",
    "instructionsChannel": "verification"
  },
