# Discord Bot Token (Required)
DISCORD_TOKEN=your_discord_bot_token_here

# MongoDB Connection String (Required)
MONGODB_URI=mongodb://username:password@localhost:27017/jmf-bot

# Environment (development or production)
NODE_ENV=development

# API Keys for Game Server Integration
PTERODACTYL_API_KEY=your_pterodactyl_api_key_here
PTERODACTYL_API_URL=https://panel.jmfhosting.com/api

# Logging Level (debug, info, warn, error)
LOG_LEVEL=info

# Bot Owner ID
OWNER_ID=your_discord_id_here

# Guild ID (for development and testing)
GUILD_ID=your_guild_id_here

# Bot Client ID (for slash commands)
CLIENT_ID=your_bot_client_id_here 